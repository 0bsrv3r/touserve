<!-- imagebox -->

<script> let starCounts = []; </script>

<!-- Tours -->
<section class="flat-row pd-imagebox v4">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-3">
                <div class="dividers dividers-title-section"></div>
                <div class="title-section color-black style3 gallery">
                    <a class="number-slide" href="/accommodations">01</a>
                    <h3 class="top-section">Recommended for you</h3> <!-- /.top-section -->
                    <h2 class="title">Our Adventures & <span>Tours</span> </h2><!-- ./title -->
                </div>
            </div><!-- /.col-md-4 -->
            
            <% for(tour of tours){ %>
            <div class="col-lg-4 col-md-3">
                <div class="flat-imagebox style2 v2">
                    <div class="item">
                        <div class="imagebox">
                            <div class="box-wrap">
                                <div class="box-image">
                                    <a href="/tour-details/<%= tour.id %>"><img src="/<%= tour.images[0] %>" alt="img"></a>
                                    <div class="overlay"></div> <!-- /.overlay -->
                                    <div class="imagebox-content">
                                        <ul class="imagebox-link">
                                            <li class="icon-s FromLeft">
                                                <a href="/tour-details/<%= tour.id %>">
                                                    <button class="flat-button">Book Now</button>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="box-content">
                                    <h4><%= tour.title %></h4>
                                    <div class="money"><%= tour.price %> <span> <%= tour.currency %></span></div>
                                    <div class="sub-title"><%= tour.duration %> days</div>
                                    <div class="wrap-star s2 v1">
                                        <ul id="review-stars" class="star v1" required="required">
                                            <li data-value="1"></li>
                                            <li data-value="2"></li>
                                            <li data-value="3"></li>
                                            <li data-value="4"></li>
                                            <li data-value="5"></li>
                                        </ul>
                                        <script> 
                                            starCounts.push(Math.round('<%= tour.dataValues.totalStars %>')); 
                                        </script>
                                    </div>
                                </div>
                            </div> <!-- /.box-wrap -->
                        </div> <!-- /.imagebox -->
                    </div> <!-- /.item -->
                    <div class="dividers dividers-imagebox s2 v2"></div>
                </div> <!-- /.flat-imagebox -->
            </div> <!-- /.col-md-4 -->
            <% } %>   
            
        </div><!-- /.row -->
    </div><!-- /.container -->
</section>

