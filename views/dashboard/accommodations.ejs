            <!-- Start right Content here -->
            <!-- ============================================================== -->                      
            <div class="content-page">
                <!-- Start content -->
                <div class="content">
                    <div class="container">

                        <!-- Page-Title -->
                        <div class="row" style="margin-bottom: 20px;">
                            <div class="col-sm-12">
                                <div style="display: flex; justify-content: space-between;">
                                    <h4 class="page-title">Your Accommodations</h4>
                                    <div class="btn-group pull-right">
                                        <a href="/dashboard/accommodations/create" class="btn btn-default dropdown-toggle waves-effect waves-light" aria-expanded="false">Create New Accommodation</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <div class="row"> 
                            <div class="col-lg-12"> 
                                <div class="card-box table-responsive">
                                    <table id="datatable" class="table table-striped table-bordered">
                                        <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Title</th>
                                            <th>Price</th>
                                            <th>Checkin - Checkout</th>
                                            <th>Amenities</th>
                                            <th>Room Count</th>
                                            <th>Location</th>
                                            <th>Actions</th>
                                        </tr>
                                        </thead>
        
                                        <tbody>
                                        <% for(let i=0; i < accommodations.length; i++){ %>
                                        <tr>
                                            <td><%= i+1 %></td>
                                            <td><%= accommodations[i].title %></td>
                                            <td><%= accommodations[i].price %> <%= accommodations[i].currency %></td>
                                            <td><%= accommodations[i].in %> - <%= accommodations[i].out %></td>
                                            <td><%= accommodations[i].amenities %></td>
                                            <td><%= accommodations[i].roomCount %></td>
                                            <td><%= accommodations[i].city %>, <%= accommodations[i].country %></td>
                                            <td>
                                                <a href="/dashboard/accommodations/update/<%= accommodations[i].id %>" style="padding: 5px;"><i class="fa fa-pencil" style="color: #29b6f6;"></i></a>
                                                <a href="/dashboard/accommodations/delete/<%= accommodations[i].id %>" style="padding: 5px;"><i class="fa fa-trash-o" style="color: #f05050;;"></i></a>
                                            </td>
                                        </tr>
                                        <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div> 
                        </div>
                         <!-- end row -->
            		</div> <!-- container -->
                               
                </div> <!-- content -->

                <footer class="footer">
                    Â© 2016. All rights reserved.
                </footer>

            </div>
            <!-- ============================================================== -->
            <!-- End Right content here -->
            <!-- ============================================================== -->