                            <form action="/profile/accommodations/create"  method="post" enctype="multipart/form-data" novalidate class="contact-form" id="contactform">
                                <div class="wrap-input">
                                    <div class="col-md-6">
                                        <p class="profile-input">                                      
                                            <input id="demo1" class="demo1 form-input-size" name="images" type="file" multiple placeholder="Select Files" />
                                            <% if (errors.image) { %>
                                                <p style="color: red;"><%= errors.image %></p>
                                            <% } %> 
                                        </p>
                                    </div>
                                    <div class="col-md-6">
                                        <p class="profile-input">                                      
                                            <input class="form-input-size" type="text" name="title" placeholder="Title*" aria-required="true" size="30" value="" id="author">
                                        </p>
                                        <% if (errors.title) { %>
                                            <p style="color: red;"><%= errors.title %></p>
                                        <% } %> 
                                    </div>
                                    <div class="col-md-6">
                                        <p class="profile-input">
                                            <select class="form-input-size" name="accommodationType">
                                                <option disabled selected>Accommodation Type</option>
                                                <option value="offroad">Home</option>
                                                <option value="hunting">Aparment</option>
                                            </select>
                                        </p>
                                    </div>
                                    <div class="col-md-6">
                                        <p class="profile-input">
                                            <select class="form-input-size" id="language" name="amenities" data-placeholder="Amenities" multiple data-multi-select>
                                                <option>Kitchen</option>
                                                <option>Pool</option>
                                                <option>Table Tennis</option>
                                                <option>Wifi</option>
                                                <option">TV</option>
                                                <option">Kitchen</option>
                                                <option">Washing Machine</option>
                                                <option">Parking</option>
                                            </select>
                                        </p>
                                        <% if (errors.amenities) { %>
                                            <p style="color: red;"><%= errors.amenities %></p>
                                        <% } %> 
                                    </div>
                                    <div class="col-md-3">
                                        <div class="profile-input input-group date form_time" data-date="" data-date-format="hh:ii" data-link-field="dtp_input2" data-link-format="hh:ii">
                                            <input class="form-control form-input-size" name="in" placeholder="In" size="16" type="text" value="" readonly>
                                            <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
                                            <span class="input-group-addon"><span class="glyphicon glyphicon-time"></span></span>
                                        </div>
                                        <% if (errors.in) { %>
                                            <p style="color: red;"><%= errors.in %></p>
                                        <% } %> 
                                    </div>
                                    <div class="col-md-3">
                                        <div class="profile-input input-group date form_time" data-date="" data-date-format="hh:ii" data-link-field="dtp_input2" data-link-format="hh:ii">
                                            <input class="form-control form-input-size" name="out" placeholder="Out" size="16" type="text" value="" readonly>
                                            <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
                                            <span class="input-group-addon"><span class="glyphicon glyphicon-time"></span></span>
                                        </div>
                                        <% if (errors.out) { %>
                                            <p style="color: red;"><%= errors.out %></p>
                                        <% } %> 
                                    </div>
