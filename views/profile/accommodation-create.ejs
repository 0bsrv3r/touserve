                                                <option>1</option>
                                                <option>2</option>
                                                <option>3</option>
                                                <option>4</option>
                                                <option>5</option>
                                                <option>6</option>
                                            </select>
                                        </p>
                                    </div>
                                    <div class="col-md-3">
                                        <p class="profile-input">
                                            <select class="form-input-size" name="bedCount">
                                                <option disabled selected>Bed Count</option>
                                                <option>1</option>
                                                <option>2</option>
                                                <option>3</option>
                                                <option>4</option>
                                                <option>5</option>
                                                <option>6</option>
                                            </select>
                                        </p>
                                    </div>
                                    <div class="col-md-3">
                                        <p class="profile-input">
                                            <select class="form-input-size" name="bathCount">
                                                <option disabled selected>Bathroom Count</option>
                                                <option>1</option>
                                                <option>2</option>
                                                <option>3</option>
                                                <option>4</option>
                                                <option>5</option>
                                                <option>6</option>
                                            </select>
                                        </p>
                                    </div>
                                    <div class="col-md-3">
                                        <p class="profile-input">
                                            <select class="form-input-size" name="guestCount">
                                                <option disabled selected>Guest Count</option>
                                                <option>1</option>
                                                <option>2</option>
                                                <option>3</option>
                                                <option>4</option>
                                                <option>5</option>
                                                <option>6</option>
                                            </select>
                                        </p>
                                    </div>
                                    <div class="col-md-6">
                                        <p class="profile-input">
                                            <select class="form-input-size" name="roomType">
                                                <option disabled selected>Room Type</option>
                                                <option>Entire Home</option>
                                                <option>Private Room</option>
                                            </select>
                                        </p>
                                    </div>
                                    <div class="col-md-6">
                                        <p class="profile-input">
                                            <select class="form-input-size" id="language" name="rules" data-placeholder="Rules" multiple data-multi-select>
                                                <option>No Smoking</option>
                                                <option>Pets Allowed</option>
                                            </select>
                                        </p>
                                        <% if (errors.rules) { %>
                                            <p style="color: red;"><%= errors.rules %></p>
                                        <% } %> 
                                    </div>
                                    <div class="col-md-4">
                                        <p class="profile-input">
                                            <select class="form-input-size" name="promotions">
                                                <option disabled selected>Promotions</option>
                                                <option>10</option>
                                                <option>20</option>
                                                <option>30</option>
                                            </select>
                                        </p>
                                    </div>
                                    <div class="col-md-4">
                                        <p class="profile-input">
                                            <select class="form-input-size" name="weeklyDiscount">
                                                <option disabled selected>Weekly Discount</option>
                                                <option>10</option>
                                                <option>20</option>
                                                <option>30</option>
                                            </select>
                                        </p>
                                    </div>
