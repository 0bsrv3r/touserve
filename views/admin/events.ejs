            <div class="content-page">
                <!-- Start content -->
                <div class="content">
                    <div class="container">

                        <!-- Page-Title -->
                        <div class="row" style="margin-bottom: 20px;">
                            <div class="col-sm-12">
                                <div style="display: flex; justify-content: space-between;">
                                    <h4 class="page-title">Your Events</h4>
                                    <div class="btn-group pull-right">
                                        <a href="/admin/events/create" class="btn btn-default dropdown-toggle waves-effect waves-light" aria-expanded="false">Create New Event</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <div class="row"> 
                            <div class="col-lg-12"> 
                                <div class="card-box table-responsive">
                                    <table id="datatable" class="table table-striped table-bordered">
                                        <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Title</th>
                                            <th>Description</th>
                                            <th>Location</th>
                                            <th>Date</th>
                                            <th>Actions</th>
                                        </tr>
                                        </thead>
        
                                        <tbody>
                                        <% for(let i=0; i < events.length; i++){ %>
                                        <tr>
                                            <td><%= i+1 %></td>
                                            <td><%= events[i].title %></td>
                                            <td><%= events[i].description %></td>
                                            <td><%= events[i].country %>, <%= events[i].city %></td>
                                            <td><%= events[i].date %></td>
                                            <td>
                                                <a href="/admin/events/update/<%= events[i].id %>" style="padding: 5px;"><i class="fa fa-pencil" style="color: #29b6f6;"></i></a>
                                                <a href="/admin/events/delete/<%= events[i].id %>" style="padding: 5px;"><i class="fa fa-trash-o" style="color: #f05050;;"></i></a>
                                            </td>
                                        </tr>
                                        <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div> 
                        </div>
                         <!-- end row -->
            		</div> <!-- container -->
                               
                </div> <!-- content -->

                <footer class="footer">
                    Â© 2016. All rights reserved.
                </footer>

            </div>
