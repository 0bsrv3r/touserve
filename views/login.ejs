<!DOCTYPE html>
<!--[if IE 8 ]><html class="ie" xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US" lang="en-US"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--><html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US" lang="en-US"><!--<![endif]-->
<head>
    <!-- Basic Page Needs -->
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    <title>Touserve</title>
    <meta name="author" content="themesflat.com">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <link rel="stylesheet" type="text/css" href="/upload/stylesheets/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/upload/stylesheets/style.css">
    <link rel="stylesheet" type="text/css" href="/upload/stylesheets/custom.css">
    <link rel="stylesheet" type="text/css" href="/upload/stylesheets/responsive.css">
    

    <!-- Favicon and touch icons  -->
    <link href="/upload/icon/apple-touch-icon-48-precomposed.png" rel="apple-touch-icon-precomposed">
    <link href="/upload/icon/apple-touch-icon-32-precomposed.png" rel="apple-touch-icon-precomposed">
    <link href="/upload/icon/favicon.png" rel="shortcut icon">

</head>                                 
<body class="header_sticky">
    
    <section class="flat-row page-login bg-theme pdtitle-section v13">
        <div class="container">
            <div class="col-md-6">
                <form novalidate="" class="login-form" id="loginform" method="post" action="/auth/login">
                    <div class="title-section center color-black style5 style12">
                        <h2 class="title">Login Now</h2> <!-- ./title -->
                        <div class="sub-title">
                            <p>Please login to your account</p>
                        </div>                 
                    </div>
                    <div class="wrap-login wrap-login-register">
                        <div class="wrap-input">
                            <% if (errors.wrongAttempt) { %>
                                <p style="color: red;"><%- errors.wrongAttempt %></p>
                            <% } %> 
                            <div class="login-inputs">
                                <p class="login-input">                                      
                                    <input type="text" placeholder="User Name*" aria-required="true" size="30" value="" name="uname" id="author" >
                                </p>
                            </div>
                            <div class="login-inputs">
                                <p class="login-input">                                        
                                    <input type="password" placeholder="Password*" size="30" value="" name="password" id="password">
                                </p> 
                            </div>
                        </div> 
                        <div class="wrap-security">
                            <a href="#" title="" class="security remember">Remember Me</a>
                            <a href="#" title="" class="security forgot">Forgot Password ?</a>
                        </div>
                        <p class="form-submit">                 
                            <button class="login-submit">Login Now</button>
                        </p>
                        <div class="not-account">
                            <p>Donâ€™t have account -<a href="register" title=""> Register Now</a></p>
                        </div>
                    </div>
                    <div class="other-account">
                        <ul>
                            <li><a href="#" title="" class="flat-account acc-google">Login Via Google+</a></li>
                            <li><a href="#" title="" class="flat-account acc-face">Login Via Facebook</a></li>
                            <li><a href="#" title="" class="flat-account acc-twiter">Login Via Twitter</a></li>
                        </ul>
                    </div>
                </form>
            </div>
        </div>
    </section>
    
    <!-- Javascript -->
    <script type="text/javascript" src="/upload/javascript/jquery.min.js"></script>
    <script type="text/javascript" src="/upload/javascript/jquery.easing.js"></script>
    <script type="text/javascript" src="/upload/javascript/jquery.magnific-popup.min.js"></script> 
    <script type="text/javascript" src="/upload/javascript/jquery.flexslider-min.js"></script>  
    <script type="text/javascript" src="/upload/javascript/main.js"></script>

    <!-- Owl-Carousel -->
    <script type="text/javascript" src="/upload/javascript/owl.carousel.js"></script>
    <script type="text/javascript" src="/upload/javascript/owl.carousel-edit.js"></script>

    <!-- Photos Gallery -->
    <script type="text/javascript" src="/upload/javascript/imagesloaded.min.js"></script>  
    <script type="text/javascript" src="/upload/javascript/isotope.min.js"></script>

    <!-- Slider -->
    <script src="/upload/javascript/ninja-slider.js"></script>
    <script src="/upload/javascript/thumbnail-slider.js" type="text/javascript"></script>

    <!-- Date Time -->
    <script type="text/javascript" src="/upload/date-time/jquery/jquery-1.8.3.min.js" charset="UTF-8"></script>
    <script type="text/javascript" src="/upload/date-time/js/bootstrap-datetimepicker.js" charset="UTF-8"></script>
    <script type="text/javascript" src="/upload/javascript/custom.js"></script>
</body>
</html>